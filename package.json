{
  "name": "d3-tip",
  "version": "0.8.0-alpha.1",
  "description": "Tooltips for d3 svg visualizations",
  "keywords": [
    "d3",
    "tooltip"
  ],
  "homepage": "https://github.com/Caged/d3-tip",
  "bugs": {
    "url": "https://github.com/Caged/d3-tip/issues"
  },
  "license": "MIT",
  "author": "Justin Palmer <justin@labratrevenge.com> (http://labratrevenge.com/d3-tip)",
  "main": "lib/index.js",
  "module": "src/index",
  "jsnext": "src/index.js",
  "browser": "lib-browser/index.js",
  "directories": {
    "doc": "docs",
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Caged/d3-tip"
  },
  "scripts": {
    "circle:lint": "npm run -s lint -- --max-warnings 0 -f junit -o $CIRCLE_TEST_REPORTS/eslint/junit.xml",
    "lint": "eslint .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-commonjs": "tsc -p .",
    "build-browser-src": "webpack",
    "build-browser-min": "cross-env NODE_ENV=min webpack --display-modules",
    "build-browser-min-debug": "cross-env NODE_ENV=min-debug webpack --display-modules",
    "build-browser": "npm run build-browser-src && npm run build-browser-min",
    "build": "npm run build-commonjs && npm run build-browser",
    "watch": "cross-env NODE_ENV=watch webpack",
    "server": "python3 -m http.server"
  },
  "dependencies": {
    "d3-collection": "^1.0.1",
    "d3-selection": "^1.0.2"
  },
  "devDependencies": {
    "eslint": "^3.3.1",
    "eslint-config-airbnb-base": "^5.0.3",
    "eslint-plugin-import": "^1.14.0",
    "cross-env": "^3.1.4",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1"
  },
  "engines": {
    "node": ">=4.2.6"
  },
  "greenkeeper": {
    "branchPrefix": "greenkeeper/"
  }
}
